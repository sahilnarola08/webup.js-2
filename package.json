{
  "name": "@sme.up/webup.js",
  "author": "@sme.up",
  "description": "Smeup Universal Client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build-start": "next build && next start -p 3000",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "clean": "rimraf dist && rimraf node_modules",
    "prettier": "prettier --write --ignore-unknown **/*.ts **/*.tsx",
    "test": "jest --watch --coverage=false",
    "test:coverage:report": "rimraf target && jest --ci",
    "test:ci": "jest --ci --coverage=false",
    "react:dev": "npx link ../ketchup/packages/ketchup ../ketchup/packages/ketchup-react",
    "jenkins:react:dev": "npx link ../KetchUP-Develop-Continuum/packages/ketchup ../KetchUP-Develop-Continuum/packages/ketchup-react && npx link ../KetchUP-Develop-Continuum/packages/ketchup-react/node_modules/react ../KetchUP-Develop-Continuum/packages/ketchup-react/node_modules/react-dom",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e:open": "env-cmd --verbose -x start-server-and-test dev http://localhost:\\$PORT/\\$WEBUPJS_APP_CONTEXT cy:open",
    "e2e:run": "env-cmd --verbose -x start-server-and-test dev http://localhost:\\$PORT/\\$WEBUPJS_APP_CONTEXT cy:run"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@sme.up/ketchup": "^9.2.0",
    "@sme.up/ketchup-react": "^9.2.0",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "color": "^4.2.3",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "luxon": "^3.3.0",
    "next": "^13.4.5",
    "next-redux-wrapper": "^8.1.0",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/react-redux": "^7.1.25",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "cypress": "^12.17.1",
    "eslint": "^8.42.0",
    "eslint-config-next": "^13.4.4",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-testing-library": "^5.11.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "prettier": "latest",
    "rimraf": "^5.0.1",
    "sass": "^1.62.1",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.1.3"
  }
}
